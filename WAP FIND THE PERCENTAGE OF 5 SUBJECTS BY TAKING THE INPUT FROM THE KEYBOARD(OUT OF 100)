package S;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        System.out.println("TAKING THE INPUT FROM USER");
        int totalmarks= 500;
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter english marks");
        int a = sc.nextInt();
        System.out.println("Enter maths marks");
        int b = sc.nextInt();
        System.out.println("Enter social science marks");
        int c = sc.nextInt();
        System.out.println("Enter hindi marks");
        int d = sc.nextInt();
        System.out.println("Enter science marks");
        int e = sc.nextInt();
        int obtainedmarks = a+b+c+d+e;
        System.out.println("the sum of the obtained marks is " + obtainedmarks);
        double percentage = (obtainedmarks/(double) totalmarks) * 100;
        System.out.println("The percentage that you have obtained is" + percentage);
        
    }
}
